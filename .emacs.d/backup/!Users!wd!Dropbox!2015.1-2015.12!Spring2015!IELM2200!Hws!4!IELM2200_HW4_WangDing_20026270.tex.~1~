%----------------------------------------------------------------------------------------
%	PROBLEM 1
%----------------------------------------------------------------------------------------

% To have just one problem per page, simply put a \clearpage after each problem

\begin{homeworkProblem}                           
  \begin{enumerate}
  \item[a.]
    \begin{align*}
      D_1 &= 100 \times 12 = 1200\\
      D_2 &= \frac{900}{6} \times 12 = 1800\\
      S &= \$55\\
      H &= 2 \times 12 = \$24\\
      Q_1 &= \sqrt{\frac{2SD_1}{H}} \approx 74 \\
      Q_2 &= \sqrt{\frac{2SD_2}{H}} \approx 91\\
    \end{align*}
    The first 6-month period should have an order size of 74 while the second 6-month period should have an order size of 91.
  \item[b.]    
    For the first 6-month period:
    \begin{align*}
      Q_1' &= 50\\
      Q_1'' &= 100\\      
      D_1 &= 100 \times 12 = 1200\\
      S_{new} &= \$45\\
      H &= 2 \times 12 = \$24\\
      TC' &= \frac{Q_1'}{2}\times H + \frac{D_1}{Q_1'}\times S = 1680\\
      TC'' &= \frac{Q_1''}{2}\times H + \frac{D_1}{Q_1''}\times S = 1740\\
    \end{align*}
    The manager should still choose 74 for the first 6-month period.
    
    For the second 6-month period:
    \begin{align*}
      Q_2' &= 50\\
      Q_2'' &= 100\\      
      D_2 &= \frac{900}{6} \times 12 = 1800\\
      S_{new} &= \$ 45\\
      H &= 2 \times 12 = \$24\\
      TC' &= \frac{Q_2'}{2}\times H + \frac{D_2}{Q_2'}\times S = 2220\\
      TC'' &= \frac{Q_2''}{2}\times H + \frac{D_2}{Q_2''}\times S = 2090.11\\
    \end{align*}
    The manager should still choose 91 for the second 6-month period.
  \end{enumerate}
\end{homeworkProblem}
\clearpage
%----------------------------------------------------------------------------------------
%	PROBLEM 2
%----------------------------------------------------------------------------------------

\begin{homeworkProblem}
\begin{enumerate}
\item[a.]
  The inventory buildup rate is $200 - 80 = 120$ units per day.
\item[b.]
  $Q_0 = \sqrt{\frac{2DS}{H}}\times\sqrt{\frac{p}{p-u}} = \sqrt{\frac{2\times80\times5\times50\times300}{10}}\times\sqrt{\frac{200}{200-80}} \approx 1414$
\item[c.]
  Run time $= \frac{Q_0}{p} = \frac{1414}{200} \approx 7$

  Cycle time $= \frac{Q_0}{u} = \frac{1414}{80} \approx 18$  
\end{enumerate}
\end{homeworkProblem}
\clearpage
%----------------------------------------------------------------------------------------
%	PROBLEM 3
%----------------------------------------------------------------------------------------

\begin{homeworkProblem}
\begin{enumerate}
\item[a.]
  \begin{align*}
    D &= 20000\\
    S &= \$ 96\\
    H &= 0.6\\
    Q &= \sqrt{\frac{2SD}{H}} \approx 2530 \\
    TC &= \frac{Q}{2}\times H + \frac{D}{Q}\times S + P\times D \approx \$ 26118\\
  \end{align*}
\item[b.]
  Number of orders per year $= \frac{D}{Q} = \frac{20000}{2530} \approx 8 $
\end{enumerate}

\end{homeworkProblem}
\clearpage
%----------------------------------------------------------------------------------------
%	PROBLEM 4
%----------------------------------------------------------------------------------------

\begin{homeworkProblem}
  \begin{table}[h!]
    \centering
    \begin{tabular}{|c|c|c|}
      \hline
      \textbf{Order Size ($Q$)} & \textbf{Prob(Demand$=Q$)} & \textbf{$F(Q)$ = Prob(Demand$\leqQ$)}\\
      \hline
      0 & 15\% & 15\%\\
      1 & 35\% & 50\%\\
      2 & 30\% & 70\%\\
      3 & 20\% & 100\% \\
      \hline
    \end{tabular}
  \end{table}
  \begin{align*}
    c_s &= \$ 60 - \$ 33 = \$ 27\\
    c_e &= \$ 33 - \$ 30 = \$ 3\\
    \frac{c_s}{c_s+c_e} &= \frac{27}{27 + 3} = 0.9\\
  \end{align*}
Therefore, as 0.9 is between $70\%$ and $100\%$, we have to produce 3 wedding cakes.
\end{homeworkProblem}
\clearpage
%----------------------------------------------------------------------------------------
%	PROBLEM 5
%----------------------------------------------------------------------------------------

\begin{homeworkProblem}
  \begin{table}[h!]
    \centering
    \begin{tabular}{|c|c|c|}
      \hline
      \textbf{Order Size ($Q$)} & \textbf{Prob(Demand$=Q$)} & \textbf{$F(Q)$ = Prob(Demand$\leqQ$)}\\
      \hline
      0 & 15\% & 15\%\\
      1 & 35\% & 50\%\\
      2 & 30\% & 70\%\\
      3 & 20\% & 100\% \\
      \hline
    \end{tabular}
  \end{table}
  \begin{align*}
    c_s &= \$ 60 - \$ 33 = \$ 27\\
    c_e &= \$ 33 - \$ 30 = \$ 3\\
    \frac{c_s}{c_s+c_e} &= \frac{27}{27 + 3} = 0.9\\
  \end{align*}
Therefore, as 0.9 is between $70\%$ and $100\%$, we have to produce 3 wedding cakes.
\end{homeworkProblem}
